{
    "suggest.timeout": 5000,
    "suggest.floatEnable": true,
    "languageserver": {
     "elixirLS": {
      "command": "~/repos/elixir-ls/language_server.sh",
      "filetypes": ["elixir", "eelixir"],
      "cwd": ".",
      "settings": {
        "elixirLS": {
          "dialyzerEnabled": false
        }
      },
      "rootPatterns": ["mix.lock"]
     }
  }
}
